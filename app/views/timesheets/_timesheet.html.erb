<div class="css_cell">
  <%= timesheet.clock_in %>
</div>
<div class="css_cell">
  <%= timesheet.clock_out %>
</div>
<div class="css_cell">
  <%= link_to edit_timesheet_path(timesheet) do %>
    <i class="glyphicon glyphicon-edit"></i>
  <% end %>
</div>
<div class="css_cell">
  <%= ( (timesheet.clock_out - timesheet.clock_in) / 3600 ).round(2) %>
</div>
<div class="css_cell">
  Approved button
</div>
